{
  "extends": ["next", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "next": ["off", {}],
    "prettier/prettier": [
      "off",
      {
        "printWidth": 80,
        "singleQuote": false,
        "trailingComma": "es5",
        "semi": false,
        "tabWidth": 2
      }
    ]
  }
}
